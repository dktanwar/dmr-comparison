running: bash  -c "mkdir -p ./output/05_fltered_dmr

for i in ./output/04_dmr/*.dmr.gz
  do
  bname=\`basename \$i .dmr.gz\`
  fname=./output/05_fltered_dmr/\${bname}.filt.dmr.gz
  
  awk -F '[:\\t]' '\$5 >= 10 && \$6>=5 {print \$0}' <(zcat \$i) | pigz -11 -p 16 > \$fname
done"
